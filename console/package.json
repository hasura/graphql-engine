{
  "name": "graphql-engine-console",
  "description": "Console for Hasura GraphQL Engine",
  "author": "Hasura (https://github.com/hasura/graphql-engine)",
  "license": "Apache 2.0",
  "version": "0.1.0",
  "repository": {
    "type": "git",
